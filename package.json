{
  "name": "gatsby-stadiums-population",
  "description": "スタジアム・アリーナと人口は、日本国内の主要スタジアム・アリーナの周辺の人口を地図で表示したサイトです。",
  "version": "1.0.0",
  "private": true,
  "author": "cieloazul310",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby clean && gatsby build --prefix-paths",
    "develop": "gatsby develop -o -H 0.0.0.0",
    "serve": "gatsby serve -H 0.0.0.0 --prefix-paths -o",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d public",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --parser typescript --trailing-comma es5 --single-quote --write \"src/**/*.ts*\"",
    "test": "npm run type-check && npm run lint",
    "ts-node": "ts-node",
    "convert": "ts-node --files ./converter/convert.ts"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@cieloazul310/canvasmap": "^0.0.2",
    "@cieloazul310/population-calculator": "^0.0.1",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@turf/turf": "^6.3.0",
    "@types/d3": "^6.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-virtualized": "^9.21.10",
    "clsx": "^1.1.1",
    "d3": "^6.3.1",
    "gatsby": "^3.0.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-canonical-urls": "^2.5.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-theme-aoi": "^1.2.2",
    "gatsby-transformer-json": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "gatsby-transformer-yaml": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-swipeable-views": "^0.13.9",
    "react-virtualized": "^9.21.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "yaml": "^1.10.0"
  }
}
